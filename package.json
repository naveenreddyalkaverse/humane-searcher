{
  "name": "humane-searcher",
  "version": "2.0.0",
  "description": "Searcher module for Humane Discovery",
  "repository": "git@github.com:naveenreddyalkaverse/humane-searcher.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "publishConfig":{
    "registry":"http://artifactory.newshunt.com/nexus/content/repositories/npm-snapshots/"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.4",
    "events": "^1.1.0",
    "humane-node-commons": "^1.1.4",
    "joi": "^8.0.5",
    "lodash": "^4.6.1",
    "md5": "^2.1.0",
    "performance-now": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli" : "^6.10.1",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-node5": "^11.0.1",
    "eslint": "^2.5.3",
    "eslint-config-360fy": "^1.1.2",
    "eslint-plugin-react": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
